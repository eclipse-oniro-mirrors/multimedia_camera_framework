+        napi_set_named_property(env_, result[PARAM0], "code", propValue);
+    }
+    ExecuteCallbackNapiPara callbackNapiPara { .recv = nullptr, .argc = ARGS_ONE, .argv = result, .result = &retVal };
+    ExecuteCallback(eventName, callbackNapiPara);
